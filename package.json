{
  "name": "formine.js",
  "version": "0.0.1",
  "description": "Formine's frontend JavaScript component, used for form building and rendering",
  "main": "./dist/cdn-node.js",
  "scripts": {
    "build": "esbuild ./src/builds/cdn.js --jsx-factory=h --jsx-fragment=Fragment --bundle --minify --sourcemap --outfile=./dist/cdn.js",
    "watch": "esbuild ./src/builds/cdn.js --jsx-factory=h --jsx-fragment=Fragment --bundle --minify --sourcemap --watch --outfile=./dist/cdn.js",
    "build-node": "esbuild ./src/index.js --bundle --platform=node --outfile=./dist/cdn-node.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waterbuckit/formine.js.git"
  },
  "author": "Adam Royal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waterbuckit/formine.js/issues"
  },
  "homepage": "https://github.com/waterbuckit/formine.js#readme",
  "devDependencies": {
    "esbuild": "^0.14.10",
    "preact": "^10.6.4"
  }
}
